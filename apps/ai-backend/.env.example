# Application
ENVIRONMENT=development
API_V1_PREFIX=/api/v1
APP_NAME=Hasselize AI Backend
APP_VERSION=0.1.0

# Server
HOST=0.0.0.0
PORT=8000
WORKERS=1

# GPU
CUDA_VISIBLE_DEVICES=0
GPU_MEMORY_FRACTION=0.9
TORCH_COMPILE=false

# Model Settings
MODEL_NAME=Tongyi-MAI/Z-Image-Turbo
MODEL_CACHE_DIR=/app/cache/models
LORA_CACHE_DIR=/app/cache/lora

# Inference Settings
INFERENCE_STEPS=4
NUM_INFERENCE_STEPS=8
GUIDANCE_SCALE=1.0
WIDTH=512
HEIGHT=512

# Supabase
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
PROMPTS_TABLE=prompts

# Cloudflare R2
R2_ENDPOINT_URL=https://your-account.r2.cloudflarestorage.com
R2_ACCESS_KEY_ID=your-key
R2_SECRET_ACCESS_KEY=your-secret
R2_BUCKET_NAME=hasselize-images
R2_PUBLIC_DOMAIN=https://images.hasselize.com  # Optional: Custom domain for public access

# Security
SECRET_KEY=your-secret-key-change-this-in-production
ALLOWED_ORIGINS=http://localhost:3000,https://hasselize.com

# Logging
LOG_LEVEL=INFO
JSON_LOGS=true
